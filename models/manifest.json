{
  "version": "0.1.0",
  "models": [
    {
      "id": "symptom-transformer",
      "type": "onnx",
      "path": "models/symptom/transformer_model.onnx",
      "description": "DistilBERT transformer model for symptom text analysis. Part of hybrid system with XGBoost.",
      "input": ["input_ids", "attention_mask"],
      "output": ["logits"],
      "sizeMB": 250
    },
    {
      "id": "symptom-xgboost",
      "type": "json",
      "path": "models/symptom/xgboost_model.json",
      "description": "XGBoost model for structured symptom features. Part of hybrid system with Transformer.",
      "input": ["float_input"],
      "output": ["probabilities"],
      "sizeMB": 5
    },
    {
      "id": "image-diagnostic",
      "type": "tflite",
      "path": "models/image/model.tflite",
      "description": "EfficientNet-Lite CNN for rash/wound detection.",
      "input": ["image_tensor"],
      "output": ["visual_condition_probabilities"],
      "sizeMB": 8.2
    },
    {
      "id": "translator",
      "type": "onnx",
      "path": "models/translator/model.onnx",
      "description": "DistilBERT/TLUnified simplification + translation head.",
      "input": ["medical_text"],
      "output": ["simplified_text"],
      "sizeMB": 25.0
    }
  ]
}

